def silnia(g):
    wynik=0
    for i in range(1,g+1,1):
        wynik+=i
    return wynik
def row_sum_odd_numbers(n):
    wynik=0
    if n==1:
        wynik=1
    else:
        start=silnia(n-1)*2+1
        for i in range((silnia(n-1)*2)+1,silnia(n-1)*2+2*n,2):
            wynik+=i
    return wynik
