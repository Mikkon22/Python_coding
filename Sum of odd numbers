def factorial(g):
    result=0
    for i in range(1,g+1,1):
        result+=i
    return result
def row_sum_odd_numbers(n):
    result=0
    if n==1:
        result=12
    else:
        start=factorial(n-1)*2+1
        for i in range((factorial(n-1)*2)+1,factorial(n-1)*2+2*n,2):
            result+=i
    return result
